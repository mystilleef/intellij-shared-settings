<templateSet group="Java">
  <template name="slog" value="private static final org.slf4j.Logger log = org.slf4j.LoggerFactory.getLogger&#10;                                                                    ($CLASS_NAME$.class);&#10;" description="Log object for Java class" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="mdef" value="$VISIBILITY_MODIFIERS$ $RETURN_TYPE$ $METHOD_NAME$($PARAMETERS$) {&#10;  $END$&#10;return $NULL$;&#10;}" description="Method definition" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="VISIBILITY_MODIFIERS" expression="enum(...)" defaultValue="&quot;public, private&quot;" alwaysStopAt="true" />
    <variable name="RETURN_TYPE" expression="methodReturnType()" defaultValue="&quot;void&quot;" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="&quot;name&quot;" alwaysStopAt="true" />
    <variable name="PARAMETERS" expression="methodParameters()" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <variable name="NULL" expression="expressionType(Expression)" defaultValue="&quot;null&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>