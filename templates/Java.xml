    <variable name="PARAMETERS" expression="methodParameters()" defaultValue="&quot;    &quot;" alwaysStopAt="true" />
    <variable name="RETURN_VALUE" expression="variableOfType(RETURN_TYPE)" defaultValue="&quot;java.lang.Object&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="void" value="$VISIBILITY_MODIFIERS$ void $METHOD_NAME$($PARAMETERS$) {&#10;  $END$&#10;}" description="Void method definition" toReformat="true" toShortenFQNames="true" useStaticImport="true">
