<templateSet group="Java">
  <template name="slog" value="private static final org.slf4j.Logger log = org.slf4j.LoggerFactory.getLogger&#10;                                                                    ($CLASS_NAME$.class);&#10;" description="Log object for Java class" toReformat="true" toShortenFQNames="true" useStaticImport="true">
