  <template name="mdef" value="$VISIBILITY_MODIFIERS$ $RETURN_TYPE$ $METHOD_NAME$($PARAMETERS$) {&#10;  $END$&#10;return $RETURN_VALUE$;&#10;}" description="Method definition" toReformat="true" toShortenFQNames="true" useStaticImport="true">
